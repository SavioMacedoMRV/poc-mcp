{
  "name": "hubformas-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@microsoft/applicationinsights-react-js": "^17.2.0",
    "@microsoft/applicationinsights-web": "^3.2.2",
    "axios": "^1.7.2",
    "lottie-react": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.11",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "start": "npm-run-all -p server dev",
    "server": "npx ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" src/server.dev.ts",
    "dev": "webpack serve --open --mode=development",
    "build": "webpack --mode=production",
    "build:qa": "webpack --mode=production --env QA",
    "test": "jest --coverageReporters=text-summary",
    "test:coverage": "jest",
    "test:ci": "jest src --coverage --coverageReporters=cobertura --coverageReporters=lcov",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "css-loader": "^6.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^7.2.0",
    "jest-transform-stub": "^2.0.0",
    "jest-transformer-svg": "^2.0.2",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
