{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "start-dev-server",
      "type": "npm",
      "script": "start",
      "isBackground": true,
      "problemMatcher": {
        "owner": "webpack",
        "pattern": {
          "regexp": "error",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "webpack compiled",
          "endsPattern": "webpack compiled successfully"
        }
      }
    }
  ]
}